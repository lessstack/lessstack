{
  "name": "lessstack",
  "version": "0.1.6",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run build && turbo run dev --parallel",
    "lint": "turbo run lint",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/config-lerna-scopes": "^17.0.2",
    "@lessstack/eslint-config": "workspace:*",
    "@lessstack/prettier-config": "workspace:*",
    "eslint": "^8.2.0",
    "husky": "^8.0.0",
    "prettier": "^2.4.1",
    "turbo": "^1.6.2",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.5.0"
}
