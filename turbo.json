{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "dev": {
      "cache": false,
      "outputs": ["build/**"]
    },
    "format": {
      "dependsOn": ["@lessstack/eslint-config#build"]
    },
    "lint": {
      "dependsOn": ["@lessstack/eslint-config#build"]
    }
  }
}
