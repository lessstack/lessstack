{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"],
      "cache": false
    },
    "lint": {
      "outputs": []
    }
  }
}
